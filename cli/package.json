{
	"name": "cmdt-cli",
	"version": "0.1.4",
	"description": "",
	"type": "module",
	"scripts": {
		"build": "pkgroll",
		"lint": "biome check",
		"typecheck": "tsc --noEmit",
		"package": "rm -rf bin && mkdir bin && pkg dist/index.cjs --output bin/cmdt --target node22-linux-arm64,node22-macos-arm64,node22-win-arm64",
		"start": "tsx src/index.ts",
		"test": "vitest --run"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
