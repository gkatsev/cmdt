		"@types/cli-progress": "^3.11.6",
		"@types/node": "^22.13.10",
		"@types/pngjs": "^6.0.5",
		"@types/resemblejs": "^4.1.3",
		"@types/xml2js": "^0.4.14",
		"@yao-pkg/pkg": "^6.3.2",
		"axios": "^1.8.3",
		"cli-progress": "^3.12.0",
		"cmdt-shared": "workspace:cmdt-shared",
		"commander": "^13.1.0",
		"deepmerge": "^4.3.1",
