		"jimp": "^1.6.0",
		"json-diff-ts": "^4.2.0",
		"memfs": "^4.36.3",
		"mkdirp": "^3.0.1",
		"pixelmatch": "^7.1.0",
		"pkgroll": "^2.11.2",
		"pngjs": "^7.0.0",
		"rimraf": "^6.0.1",
		"tsr": "^1.3.4",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"vitest": "^3.1.1",
		"winston": "^3.17.0",
